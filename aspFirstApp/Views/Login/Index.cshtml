
    @using aspFirstApp.Models;

@model List<aspFirstApp.Models.Student>


@{
    ViewBag.Title = "Index";
    }
@{
    Layout = "~/Views/Shared/LoginLayout.cshtml";
}
@{
    if(Session["id"]!=null)
    {
        Response.Redirect("~/Admin/Home");
    }
    else if(Session["name"] !=null)
    {

        Response.Redirect("~/User/Index");
    }
}
<div class="col-lg-8 col-lg-offset-2" id="bounce">
    <div class="panel panel-success">
        <div class="panel-heading">
            <h3 class="panel-title"><a href="">Sing In</a></h3>
        </div>
        <div class="panel-body">
            <form role="form" class="form-horizontal" action="/Login/signInData" onsubmit="return validate()" method="post" name="form">
                <div class="form-group has-success" id="c">
                    <label for="name" class="col-lg-2 control-label">Email</label>
                    <div class="col-lg-5">
                        <input type="text" class="form-control" name="email" id="email" placeholder="First name"
                               onblur="validateEmail()">
                    </div>
                    <p id="error5"></p>
                </div>
                <div class="form-group has-success">
                    <label for="pass" class="col-lg-2 control-label">Password</label>
                    <div class="col-lg-5">
                        <input type="password" name="pass" id="pass" class="form-control" placeholder="Password" onkeyup="validatepass()">
                    </div>
                    <p id="error6"></p>
                </div>
                <div class="form-group ">
                    <div class="col-lg-4 col-lg-offset-2 btn-group">
                        <input type="submit" value="Sign In" class="btn btn-success ">
                        <input type="reset" class="btn btn-default ">
                    </div>
                </div>
            </form>
        </div>
</div>
</div>